# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = false

# Unix-style newlines with a newline ending every file
charset = utf-8

# 2 space indentation
indent_style = space
indent_size = 2
